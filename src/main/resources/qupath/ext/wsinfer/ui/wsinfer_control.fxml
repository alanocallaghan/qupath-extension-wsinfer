<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="206.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="qupath.ext.wsinfer.ui.WSInferController">
   <children>
      <ChoiceBox fx:id="modelChoiceBox" layoutX="15.0" layoutY="15.0" />
      <Button fx:id="runButton" layoutX="500.0" layoutY="15.0" onAction="#runInference" text="%ui.run-model" disable="true"/>
      <Button fx:id="forceRefreshButton" layoutX="380.0" layoutY="15.0" mnemonicParsing="false" onAction="#forceRefresh" text="%ui.force-download" />
      <Button layoutX="385.0" layoutY="50.0" mnemonicParsing="false" text="%ui.open-measurement-maps" onAction="#openMeasurementMaps"/>
   </children>
</AnchorPane>
